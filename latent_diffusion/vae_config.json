{
    "model": {
            "name": "Subfolder name to store the training results",
            "comment": "Any comment you like", 
            "load_name": "if not empty, weights will be loaded from this path",
            "img_ch": 3, 
            "enc_init_channels": 8,
            "latent_dim": 4,
            "ch_mult": [1, 2, 4, 8], 
            "group_norm": 4,
            "resnet_stacks": 2,
            "embed_dim": null,
            "down_mode": "avg",
            "down_kern": 2,
            "down_attn": [0, 1, 2, 3],
            "up_mode": "nearest",
            "up_scale": 2,
            "up_attn": [0, 1, 2, 3],
            "attn_heads": 1,
            "attn_dim": 16,
            "eps": 1e-7,
            "scaling_factor": 0.18215,
            "dec_tanh_out": true
            }, 
    "dataset": {
            "image_size": 128, 
            "img_resize": true, 
            "use_subset": false,
            "location": "/path/to/dataset/folder with subfolders for each class"
            },
    "results": 
            {"root": "root/folder/for/the/model"},
    "loss":{
            "kl_weight":  1e-4,
            "nll_weight": 1e-4,
            "logvar_init": 0.1,
            "pixelloss_weight": 1.0,
            "perceptual_weight": 5.0,
            "pixel_loss": "l1",
            "lpips_net": "vgg",
            "disc_weight": 0.5,
            "disc_factor": 1e-1,
            "disc_loss_fn": "hinge",
            "disc_start": 2500
            },
    "discriminator":{
            "enabled": true,
            "load_name": "if not empty, weights will be loaded from this path",
            "disc_pretrain_epochs": null,
            "disc_train_batch": 256,
            "disc_grad_acc": 1,
            "disc_in_channels": 3,
            "disc_num_layers": 3,
            "disc_ndf": 8,
            "use_actnorm": false},         
    "training": {
            "chkpt" : "if not empty, weights will be loaded from this path",
            "fp16": "bf16",
            "max_norm": 1,
            "compile": true,
            "num_epochs": 1,
            "grad_step_acc": 1,
            "batch_size": 8,
            "dataloader_workers": 12,
            "sample_every": 1000,
            "save_every": 2000,
            "save_snapshot": true,
            "init_scale": 65536,
            "growth_interval": 1,
            "growth_factor": 2,
            "backoff_factor": 0.5,
            "last_layer" : true
            }, 
    "optimizer": {
            "lr": 3e-4,
            "betas": [0.5, 0.9],
            "eps": 1e-7,
            "weight_decay": 1e-5}, 
    "lr_scheduler": {
            "enabled": true,
            "type": "onecyclelr",
            "max_lr": 4e-3,
            "pct_start": 0.45,
            "anneal_strategy": "cos",
            "cycle_momentum": false,
            "base_momentum": 0.85,
            "max_momentum": 0.90,
            "div_factor": 8,
            "final_div_factor": 16,
            "three_phase": true
            },
    "optimizer_d": {
            "lr": 3e-4,
            "betas": [0.5, 0.9],
            "eps": 1e-7,
            "weight_decay": 1e-5}, 
    "lr_scheduler_d": {
            "enabled": true,
            "type": "onecyclelr",
            "max_lr": 4e-3,
            "pct_start": 0.45,
            "anneal_strategy": "cos",
            "cycle_momentum": false,
            "base_momentum": 0.85,
            "max_momentum": 0.90,
            "div_factor": 4,
            "final_div_factor": 8,
            "three_phase": true
            }
    }